{
	"name": "directus-extension-ai-pack",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "operation",
				"name": "dall-e-operation",
				"source": {
					"app": "src/dall-e-operation/app.js",
					"api": "src/dall-e-operation/api.js"
				}
			},
			{
				"type": "operation",
				"name": "stable-diffusion-operation",
				"source": {
					"app": "src/stable-diffusion-operation/app.ts",
					"api": "src/stable-diffusion-operation/api.ts"
				}
			},
			{
				"type": "operation",
				"name": "davinci-operation",
				"source": {
					"app": "src/davinci-operation/app.js",
					"api": "src/davinci-operation/api.js"
				}
			}
		],
		"host": "^9.21.2"
	},
	"scripts": {
		"add": "directus-extension add",
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.21.2",
		"@types/node": "^18.11.17",
		"typescript": "^4.9.4",
		"vue": "^3.2.45"
	},
	"dependencies": {
		"node-fetch": "^3.3.0",
		"openai": "^3.1.0"
	}
}
